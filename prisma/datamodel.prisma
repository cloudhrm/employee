type Employee {
  id: ID! @unique
  userid: String! @unique
  companyId: String
  firstName: String!
  lastName: String!
  livingLocationText: String
  livingplaceId: Int
  about: String
  workSince: Int
  skills: [EmployeeSkill!]!
  links: [Link!]!
  experience: [Experience!]!
  education: [Education!]!
}

type Skill {
  id: ID! @unique
  name: String!
}

type EmployeeSkill {
  id: ID! @unique
  employee: Employee!
  skill: Skill!
  years: Int
}

type Link {
  id: ID! @unique
  employee: Employee!
  link: String!
}

type Experience {
  id: ID! @unique
  position: String!
  company: String!
  companyId: String
  locationText: String
  locationid: Int
  fromYear: Int
  toYear: Int
  current: Boolean!
}

type Education {
  id: ID! @unique
  employee: Employee!
  fromYear: Int!
  toYear: Int
  degree: String
  fieldOfStudy: String
  school: String
  locationText: String
  locationId: Int
  notes: String
  current: Boolean!
}
